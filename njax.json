{
    "app_name":"n-glass",
    "frameworks":{
    },
    "models":{
        "symbol":{
            "uri_prefix":"",
            "fields":{
                "namespace":"namespace",
                "name":"string",
                "desc":"md",
                "tags":"Object",
                "owner":{
                    "type":"ref",
                    "ref":"account",
                    "bootstrap_populate":"req.user"
                }

            }
        },
        "image":{
            "uri_prefix":"/images",
            "fields":{
                "namespace":"namespace",
                "name":"string",
                "image":"s3-asset",
                "desc":"md",
                "clusters":["app.model.Cluster"],
                "owner":{
                    "type":"ref",
                    "ref":"account",
                    "bootstrap_populate":"req.user"
                }

            }
        },
        "cluster":{
            "parent":"image",
            "uri_prefix":"/clusters",
            "data_url":"string",
            "fields":{
                "bounds":"object",
                "x":"number",
                "y":"number",
                "rotation":"number",
                "value":"string",
                "pixels":"object"
            }
        }
    },
    "bowser":{
        "dependencies":{
            "fontawesome":"*"
        }
    },
    "package":{
        "dependencies":{
            "gm":"*",
            "mkdirp":"*",
            "canvas":"*",
            "hogan.js":"*",
            "aws-sdk":"*"
        }
    }
}